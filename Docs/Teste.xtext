SCRIPTSTEPSAND: {SCRIPTSTEPS} AndIClick | {SCRIPTSTEPS} AndIPress | {SCRIPTSTEPS} AndISelect | {SCRIPTSTEPS} AndIFill | AndINeedToWait | {SCRIPTSTEPSAND} WhenIClick | {SCRIPTSTEPSAND} WhenIPress | {SCRIPTSTEPSAND} WhenISelect | {SCRIPTSTEPSAND} WhenIFill;

AndIClick:
	'And' 'i click on' FREETEXT  'link' /* SOMETHING PARAMETRIZED */ 'which is dynamically generated and update on' ('Each Interaction') 'based on a' ('random' | 'same as type') 'strategy'
;

AndIPress:
	'And' 'i press the' FREETEXT  'button' /* SOMETHING PARAMETRIZED */ 
;

AndISelect:
	'And' 'i select' FREETEXT 'drop-down list with' /* something parametrized */ 'which is dynamically generated and update on' ('Each Interaction') 'based on a' ('random' | 'same as type') 'strategy'
;

AndIFill:
	'And' 'i fill in' FREETEXT 'input field within' /* something parametrized */ 'which is dynamically generated and update on' ('Each Interaction') 'based on a' ('random' | 'same as type') 'strategy'
;

AndINeedToWait:
	'And' 'i need to wait thinking time during' hour=HOUR 
;

WhenIClick:
	'When' 'i click' 'on' FREETEXT 'link' /* SOMETHING PARAMETRIZED */ ('which is dynamycally generated and update on') ('Each Interaction') 'based on a' ('random' | 'same as type') 'strategy'
;

WhenIPress:
	'When' 'i press' 'the' FREETEXT 'button' /* SOMETHING PARAMETRIZED */
;

WhenISelect:
	'When' 'i select' FREETEXT 'drop-down list with' /* something parametrized */ 'which is dynamically generated and update on' ('Each Interaction') 'based on a' ('random' | 'same as type') 'strategy'
;

WhenIFill:
	'When' 'i fill in' FREETEXT 'input field within' /* something parametrized */ 'which is dynamically generated and update on' ('Each Interaction') 'based on a' ('random' | 'same as type') 'strategy'
;



TRANSACTION:
	'Transaction' '{'
	name=ID
	'}'
;

