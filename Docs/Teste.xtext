// ##################################################################################################################################################################################################
// ##################################################### SCENARIO RULES #########################################################################################################################
// ##################################################################################################################################################################################################


SCENARIO:
//	'Feature' ':' FREETEXT '.'
	
	//################### WORKLOAD #############################
	'Scenario:' 'Evaluate the' name=ID 'workload for' integer1=INT 'users simultaneosly' 
	'Given' integer2=INT 'users enter the system every' time1=HOUR
	'And' integer3=INT 'users leave the system every' time2=HOUR
	'And' 'performance testing execution during' time3=HOUR	
	
	
	//################## scripts of this workload ######################
	(('When' percents1+=INT 'percent' 'of the virtual users execute the' userprofile=[USERPROFILE|ID] 'user profile:'
	'Then' percents+=INT 'percent' 'of them execute' script=[SCRIPT|ID] 'script')
	('And' percent3+=INT 'percent' 'of them execute' scripts+=[SCRIPT|ID] 'script')*)*	
;
	
USERPROFILE:
	'User profile' '{'
	name=ID
	'}'
;



// ##################################################################################################################################################################################################
// ##################################################################################################################################################################################################
// ##################################################################################################################################################################################################


// ##################################################################################################################################################################################################
// ##################################################### SCRIPT RULES #########################################################################################################################
// ##################################################################################################################################################################################################


SCRIPT:
	'Script:' FREETEXT 'based on ' testcaseidentifier=FREETEXT 'from'  name=ID '{'
	'Given' 'the' transaction1=[TRANSACTION|ID] 'transaction activity through' FREETEXT 'action, which is loaded in' (EInt | EDouble) 'seconds'
	stepswhen=SCRIPTSTEPSAND 
	(('Then' 'i will be taken to' FREETEXT  'action in the' transaction2=[TRANSACTION|ID] 'transaction activity, which is loaded in' seconds=EDouble 'seconds')
	steps+=(SCRIPTSTEPSAND)*)*
	
	
	
	'}'
;

